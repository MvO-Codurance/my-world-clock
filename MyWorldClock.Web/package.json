{
  "name": "myworldclock.web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44446 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44446 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build -c=production",
    "build:dev": "ng build -c=development",
    "build:watch": "ng build --watch -c=development",
    "test": "jest --verbose",
    "test:watch": "jest --watch-all --verbose",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.html\"",
    "lint:html": "htmlhint \"src/**/*.html\"",
    "lint:watch": "esw \"src/**/*.ts\" \"src/**/*.html\" --color --clear --watch --ext .ts,.html",
    "lint:fix": "eslint --fix \"src/**/*.ts\" \"src/**/*.html\"",
    "serve": "ng serve --open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.5",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.1.5",
    "@angular/compiler": "^15.1.5",
    "@angular/core": "^15.1.5",
    "@angular/forms": "^15.1.5",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.1.5",
    "@angular/platform-browser-dynamic": "^15.1.5",
    "@angular/platform-server": "^15.1.5",
    "@angular/router": "^15.1.5",
    "bootstrap": "^5.2.3",
    "jquery": "^3.6.3",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "^15.1.6",
    "@angular/compiler-cli": "^15.1.5",
    "@testing-library/angular": "12.1.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@types/angular": "1.8.4",
    "@types/jasmine": "4.3.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.14.0",
    "@types/prop-types": "15.7.5",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "eslint": "8.26.0",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-rxjs": "5.0.2",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-testing-library": "5.9.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "eslint-watch": "8.0.0",
    "htmlhint": "1.1.4",
    "jasmine-core": "~4.5.0",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.0.1",
    "ng-mocks": "^14.10.0",
    "typescript": "~4.9.5"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup"
  }
}